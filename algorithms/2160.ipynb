    "class Solution:\n",
    "    def minimumSum(self, num: int) -> int:\n",
    "        d = sorted([int(n) for n in str(num)])\n",
    "        return (d[0]*10 + d[2]) + (d[1]*10+d[-1])"
   ]
  },
