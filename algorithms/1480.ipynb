   "source": [
    "class Solution:\n",
    "    def runningSum(self, nums):\n",
    "        ans = []\n",
    "        s = 0\n",
    "        for n in nums:\n",
    "            s += n\n",
    "            ans.append(s)\n",
    "        return ans"
   ]
  },
