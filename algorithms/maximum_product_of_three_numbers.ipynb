   "source": [
    "class Solution:\n",
    "    def maximumProduct(self, nums) -> int:\n",
    "        nums = sorted(nums)\n",
    "        return max(nums[-1]*nums[-2]*nums[-3], nums[0]*nums[1]*nums[-1])"
   ]
