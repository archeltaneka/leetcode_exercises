    "class Solution:\n",
    "    def isPerfectSquare(self, num: int) -> bool:\n",
    "        temp = int(num ** .5)\n",
    "        print(temp)\n",
    "        if temp ** 2 == num:\n",
    "            return True\n",
    "        return False"
