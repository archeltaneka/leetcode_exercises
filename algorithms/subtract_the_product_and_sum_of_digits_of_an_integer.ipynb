   "source": [
    "class Solution:\n",
    "    def subtractProductAndSum(self, n: int) -> int:\n",
    "        s, p = 0, 1\n",
    "        for num in str(n):\n",
    "            s += int(num)\n",
    "            p *= int(num)\n",
    "        return p - s"
   ]
  },
