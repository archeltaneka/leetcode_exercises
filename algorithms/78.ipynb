   "source": [
    "class Solution:\n",
    "    def subsets(self, nums):\n",
    "        ans = [[]]\n",
    "        for n in nums:\n",
    "            ans += [a + [n] for a in ans]\n",
    "\n",
    "        return ans"
   ]
