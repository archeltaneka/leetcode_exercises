   "source": [
    "class Solution:\r\n",
    "    def lengthOfLastWord(self, s: str) -> int:\r\n",
    "        split_str = s.split()\r\n",
    "        return len(split_str[-1])"
   ],
   "outputs": [],
   "metadata": {}
  },
