   "source": [
    "class Solution:\r\n",
    "    def strStr(self, haystack: str, needle: str) -> int:\r\n",
    "        if len(needle) == 0:\r\n",
    "            return 0\r\n",
    "        return haystack.find(needle)\r\n"
   ],
   "outputs": [],
   "metadata": {}
  },
