   "source": [
    "import numpy as np\n",
    "class Solution:\n",
    "    def missingNumber(self, nums) -> int:\n",
    "        n = len(nums)+1\n",
    "        temp = np.arange(0, n)\n",
    "        \n",
    "        return sum(temp) - sum(nums)"
   ]
  },
